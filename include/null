# non responsive
if [[ -z "$WALLET_NODES_TOTAL" ]]; then
  WALLET_NODES_TOTAL="${GR:?}0${N0:?}"
fi
if [[ -z "$WALLET_NODES_STABLE" ]]; then
  WALLET_NODES_STABLE="${GR:?}0${N0:?}"
fi
if [[ -z "$WALLET_NODES_ENABLED" ]]; then
  WALLET_NODES_ENABLED="${GR:?}0${N0:?}"
fi
if [[ -z "$WALLET_NODES_INQUEUE" ]]; then
  WALLET_NODES_INQUEUE="${GR:?}0${N0:?}"
fi
if [[ -z "$IP_ADDRESS" ]]; then
  IP_ADDRESS="${GR:?}0.0.0.0${N0:?}"
fi
if [[ -z "$WALLET_VERSION" ]]; then
  WALLET_VERSION="${GR:?}0.0.0${N0:?}"
fi
if [[ -z "$WALLET_CHAIN" ]]; then
  WALLET_CHAIN="${GR:?}0${N0:?}"
fi
if [[ -z "$WALLET_HEADS" ]]; then
  WALLET_HEADS="${GR:?}0${N0:?}"
fi
if [[ -z "$WALLET_BLOCK" ]]; then
  WALLET_BLOCK="${GR:?}0${N0:?}"
fi
if [[ -z "$WALLET_PEERS_TOTAL" ]]; then
  WALLET_PEERS_TOTAL="${GR:?}0${N0:?}"
fi
if [[ -z "$WALLET_PEERS_LATEST" ]]; then
  WALLET_PEERS_LATEST="${GR:?}0${N0:?}"
fi
if [[ -z "$WALLET_PEERS_OTHER" ]]; then
  WALLET_PEERS_OTHER="${GR:?}0${N0:?}"
fi
if [[ "$WALLET_PEERS_TOTAL" -eq "0" ]]; then
  WALLET_PEERS_TOTAL="${GR:?}0${N0:?}"
fi
if [[ "$WALLET_PEERS_LATEST" -eq "0" ]]; then
  WALLET_PEERS_LATEST="${GR:?}0${N0:?}"
fi
if [[ "$WALLET_PEERS_OTHER" -eq "0" ]]; then
  WALLET_PEERS_OTHER="${GR:?}0${N0:?}"
fi
if [[ -z "$WALLET_BALANCE" ]]; then
  WALLET_BALANCE="${GR:?}0.00${N0:?}"
fi
if [[ -z "$WALLET_MEMPOOL" ]]; then
  WALLET_MEMPOOL="${GR:?}0${N0:?}"
fi
if [[ -z "$WALLET_TXCOUNT" ]]; then
  WALLET_TXCOUNT="${GR:?}0${N0:?}"
fi
if [[ -z "$RC_LOADING" ]]; then
  RC_LOADING=""
fi
if [[ -z "$WALLET_INPUTS" ]]; then
  WALLET_INPUTS="${GR:?}0${N0:?}"
fi
if [[ "$WALLET_INPUTS" -eq "0" ]]; then
  WALLET_INPUTS="${GR:?}0${N0:?}"
fi
if [[ -z "$WALLET_STAKES" ]]; then
  WALLET_STAKES="${GR:?}0${N0:?}"
fi
if [[ -z "$LAST_TX" ]]; then
  LAST_TX="0"
fi
if [[ -z "$WALLET_TXLAST" ]]; then
  WALLET_TXLAST="${N0:?}"
fi

# END
